{
  "manifest_version": 2,
  "name": "Pwned",
  "description": "Chrome extension to check if the website you're visiting or
  the credentials you're entering appear in the hibp database of public breaches",
  "version": "1.0",
  "icons": {
    "16":"/images/exclam-icon.png",
    "48":"/images/Key-icon.png",
    "128":"/images/Key-icon.png"
  },
  "browser_action": {
    "default_icon": "/images/Key-icon.png",
    "default_popup": "/html/popup.html",
    "default_title": "Pwned Checker"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": ["js/background.js"]
  }
}
